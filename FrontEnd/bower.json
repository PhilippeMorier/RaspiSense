{
  "name": "RaspiSense.FrontEnd",
  "version": "0.0.1",
  "authors": "Thorgal Hayne",
  "description": "Consumes a REST API which provides sensor information for temperature, light, pressure and humidity.",
  "license": "ISC",
  "main": "index.html",
  "keywords": [
    "sensor",
    "temperature",
    "light",
    "pressure",
    "humidity",
    "REST"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "overrides": {
    "bootstrap": {
      "main": [
        "less/bootstrap.less",
        "dist/css/bootstrap.css",
        "dist/css/bootstrap-theme.css",
        "dist/js/bootstrap.js",
        "dist/fonts/glyphicons-halflings-regular.eot",
        "dist/fonts/glyphicons-halflings-regular.svg",
        "dist/fonts/glyphicons-halflings-regular.ttf",
        "dist/fonts/glyphicons-halflings-regular.woff",
        "dist/fonts/glyphicons-halflings-regular.woff2"
      ]
    },
    "highcharts-release": {
      "main": [
        "standalone-framework.src.js",
        "highcharts.src.js"
      ]
    },
    "highcharts-ng": {
      "main": [
        "src/highcharts-ng.js"
      ]
    }
  },
  "dependencies": {
    "angular": "~1.4.0",
    "angular-route": "~1.4.0",
    "angular-resource": "~1.4.0",
    "angularjs-toaster": "~0.4.14",
    "bootstrap": "~3.3.4",
    "highcharts-release": "~4.1.6",
    "highcharts-ng": "~0.0.8"
  }
}
